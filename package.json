{
  "name": "@newkind/node-browser",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "type-check": "tsc --noEmit --strict --skipLibCheck",
    "declarations:collect": "node scripts/declarations.js",
    "build:dist:virtual:css": "node scripts/build.js --copy --platform browser && npm run build:css-module:html",
    "build:dist:virtual:css:watch": "nodemon --watch src --ext ts,js,css,html,json --exec \"node scripts/build.js --copy --platform browser && npm run build:css-module:html\"",
    "build:bundle:esm": "node scripts/bundle.js --platform browser --virtual-css --only-mjs --browser-only -o example",
    "build:dist": "node scripts/build.js --copy --platform browser",
    "build:dist:watch": "nodemon --watch src --ext ts,js,css,html,json --exec \"node scripts/build.js --copy --platform browser\"",
    "build:css-module:html": "node scripts/generate-css-module.js --html --force",
    "build:bundle": "node scripts/bundle.js --platform browser --virtual-css --browser-only -o example",
    "vendor:build:browser": "node scripts/vendor.mjs build --platform browser",
    "build:script:index": "node scripts/build.js --generate-index",
    "build:components": "node scripts/build.js --web-components",
    "build": "esbuild src/index.ts --bundle --outdir=dist --platform=browser",
    "build:watch": "esbuild src/index.ts --bundle --outdir=dist --platform=browser --watch",
    "dev": "npm run build:watch",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@chainsafe/libp2p-gossipsub": "^14.1.2",
    "@chainsafe/libp2p-noise": "^17.0.0",
    "@chainsafe/libp2p-yamux": "^8.0.1",
    "@libp2p/bootstrap": "^12.0.11",
    "@libp2p/circuit-relay-v2": "^4.1.3",
    "@libp2p/crypto": "^5.1.13",
    "@libp2p/dcutr": "^3.0.10",
    "@libp2p/floodsub": "^11.0.11",
    "@libp2p/http": "^2.0.1",
    "@libp2p/http-fetch": "^4.0.1",
    "@libp2p/http-ping": "^2.0.1",
    "@libp2p/identify": "^4.0.10",
    "@libp2p/interface": "^3.1.0",
    "@libp2p/interface-transport": "^4.0.3",
    "@libp2p/kad-dht": "^16.1.3",
    "@libp2p/logger": "^6.2.2",
    "@libp2p/peer-id": "^6.0.4",
    "@libp2p/peer-store": "^12.0.10",
    "@libp2p/ping": "^3.0.10",
    "@libp2p/pubsub-peer-discovery": "^12.0.0",
    "@libp2p/utils": "^7.0.10",
    "@libp2p/webrtc": "^6.0.11",
    "@libp2p/websockets": "^10.1.3",
    "@libp2p/webtransport": "^6.0.12",
    "@multiformats/multiaddr": "^13.0.1",
    "@multiformats/multiaddr-matcher": "^3.0.1",
    "blockstore-core": "^6.1.2",
    "datastore-core": "^11.0.2",
    "datastore-idb": "^4.0.1",
    "libp2p": "^3.1.3",
    "peer-id": "^0.16.0",
    "prom-client": "^15.1.3",
    "protons-runtime": "^5.6.0",
    "reflect-metadata": "^0.2.2",
    "uint8arrays": "^5.1.0"
  },
  "devDependencies": {
    "esbuild": "^0.27.2",
    "typescript": "^5.9.3"
  }
}
